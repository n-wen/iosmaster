{
	"name": "iosmaster",
	"displayName": "iosmaster",
	"publisher": "iosmaster",
	"description": "build and run ios app",
	"version": "0.0.8",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:*.xcworkspace"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.build",
				"title": "iosmaster: build"
			},
			{
				"command": "extension.run",
				"title": "iosmaster: run"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"bplist-parser": "^0.2.0"
	}
}
